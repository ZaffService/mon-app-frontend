{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    { 
      "src": "/BIG_DEAL/images/(.*)", 
      "dest": "/BIG_DEAL/images/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}