{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist",
        "staticFolder": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/BIG_DEAL/images/(.*)",
      "dest": "/BIG_DEAL/images/$1"
    }
  ]
}